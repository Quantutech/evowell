    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.56.0",
